<Window x:Class="Bolnica.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bolnica"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="109*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="MainFrame" Content="Frame" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792" Grid.ColumnSpan="2" Navigated="Frame_Navigated" NavigationUIVisibility="Hidden"/>
        <Button x:Name="ScheduledAppointmentsButton" Content="Scheduled Appointments" Grid.Column="1" HorizontalAlignment="Left" Margin="124,57,0,0" VerticalAlignment="Top" Width="209" Height="32" Click="Button_Click"/>
        <Button x:Name="ScheduledSurgeriesButton" Content="Scheduled Surgeries" Grid.Column="1" HorizontalAlignment="Left" Margin="124,97,0,0" VerticalAlignment="Top" Width="209" Height="32"/>
        <Button x:Name="CalendarButton" Content="Calendar" Grid.Column="1" HorizontalAlignment="Left" Margin="124,137,0,0" VerticalAlignment="Top" Width="209" Height="32"/>
        <Button x:Name="MyPatientsButton" Content="My Patients" Grid.Column="1" HorizontalAlignment="Left" Margin="124,177,0,0" VerticalAlignment="Top" Width="209" Height="32"/>
        <Button x:Name="PerscriptionsButton" Content="Perscriptions" Grid.Column="1" HorizontalAlignment="Left" Margin="124,216,0,0" VerticalAlignment="Top" Width="209" Height="32"/>
    </Grid>
</Window>
