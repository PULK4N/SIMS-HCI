<Window x:Class="Hospital.Pages.Patient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital.Pages"
        mc:Ignorable="d"
        Title="Patient" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="50" VerticalAlignment="Top">
            <Button Content="Schedule Appointment " Margin="20,10,0,0" Click="Button_Click_1" />
            <Button Content="View Scheduled Appointments " Margin="20,10,0,0" Click="Button_Click" />
        </StackPanel>
        <Frame x:Name ="Main" Margin=" 0,35,0,0 " NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
